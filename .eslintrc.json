{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/essential"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 2018,
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "rules": {
        "indent": "off",
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": "off",
        "semi": "off",
        "no-extra-semi": "off",
        "no-console": [
            "warn", {
                "allow": ["warn", "error", "info", "assert"]
            }
        ],
        "no-unused-vars": "off",
        "no-inner-declarations": "off",
        "vue/no-side-effects-in-computed-properties": "off"
    }
}